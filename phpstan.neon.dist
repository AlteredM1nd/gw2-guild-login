includes:
    - vendor/php-stubs/wordpress-stubs/wordpress-stubs.php

parameters:
    level: 5
    paths:
        - .
    excludePaths:
        - vendor/*
        - node_modules/*
    ignoreErrors:
        - '#Call to an undefined method.*#'
        - '#Access to an undefined property.*#'
        - '#Class .* not found#'
        - '#Instantiated class .* not found#'
        - '#Undefined class constant#'
        - '#Undefined function.*#'
        - '#Function .* not found#'
